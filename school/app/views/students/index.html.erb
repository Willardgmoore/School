<h1>Students</h1>
<%= link_to 'new', new_student_path %>


<% @students.each do |student| %>
<p><%= student.first_name %>
<%= student.last_name %></p>
<%= link_to 'show', student_path(student) %>
<%= link_to 'edit', edit_student_path(student) %>
<%= link_to 'delete', student, method: :delete %>
<% end %>